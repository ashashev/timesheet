  object sqlTimeCrosscup: TSQLQuery
    FieldDefs = <>
    Database = dbCon
    Transaction = sqlTran
    SQL.Strings = (
      'select'
      '*,'
      'ts.task_code || '' '' || ts.task_description task'
      'from timesheet ts'
      'where'
      '  (ts.date = :date) and'
      '  (not( (ts.time_from >= :time_to) or (ts.time_to <= :time_from) )) and'
      '  ifnull(ts.id <> :id, 1 )'
    )
    Params = <    
