      end    
      item
        DataType = ftUnknown
        Name = 'time_from'
        ParamType = ptInput
      end    
      item
        DataType = ftUnknown
        Name = 'id'
        ParamType = ptInput
      end>
    left = 168
    top = 136
  end
  object sqlWeekReport: TSQLQuery
    FieldDefs = <    
      item
        Name = 'month'
        DataType = ftString
        Precision = -1
        Size = 255
      end    
      item
        Name = 'task'
        DataType = ftString
        Precision = -1
        Size = 255
      end    
      item
        Name = 'time'
        DataType = ftLargeint
        Precision = -1
        Size = 0
      end>
    Database = dbCon
    Transaction = sqlTran
    SQL.Strings = (
      'select'
      '  strftime(''%Y-%m-01'',date(ts.date)) as month,'
      '  ts.task_code || '' '' || ts.task_description task,'
      '  sum(ifnull(ts.time_to - ts.time_from,0)) as time'
      'from timesheet ts'
      'left join categories c on c.id = ts.category'
      'where ts.date between :datefrom and :dateto and (ifnull(c.used,0) != 0)'
      'group by strftime(''%m'',date(ts.date)), (ts.task_code || '' '' || ts.task_description)'
      'order by month, task'
      ''
    )
    Params = <    
      item
        DataType = ftUnknown
        Name = 'datefrom'
        ParamType = ptInput
